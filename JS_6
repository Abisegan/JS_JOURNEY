for (var i = 1; i <= 5; i++) {
    console.log(i);
}
console.log("Counting completed!");


for (var i = 5; i > 0; i--) {
    console.log(i);
}
console.log("Countdown finished!");

var i = 1;
while (i <= 5) {
    console.log(i);
    i++;
}
console.log("Counting completed!");

var j = 5;
while (j > 0) {
    console.log(2023 - j);
    j--;
}
console.log("Countdown finidhed!");