var age = 10;
if (age >= 65) {
    console.log("You get your incom from your pension");
} else if (age < 65 && age >= 18) {
    console.log("Each month you get a salary");
} else if (age < 18) {
    console.log("You get an allowace");
} else {
    console.log("The value of the age vaiable is not numerical");
}


var day = "sunday";
switch (day) {
    case "monday":
        console.log("run");
        break;

    case "tuesday":
        console.log("jump");
        break;
        
    case "wednesday":
        console.log("sing");
        break;
        
    case "thurstday":
        console.log("eat");
        break;
        
    case "friday":
        console.log("claim");
        break;
        
    default:
        console.log("There is no such day");   
}